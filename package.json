{
  "name": "beetask-front",
  "version": "0.0.0",
  "description": "beeTask",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -c local-pre --hmr --proxy-config proxy.conf.json --disable-host-check",
    "start-host": "ng serve -c local-pre --hmr --proxy-config proxy.conf.json --host 0.0.0.0 --disable-host-check",
    "start-local": "ng serve --c local --hmr --proxy-config proxy.conf.json",
    "build-pre": "ng build -c pre --progress=false --aot=true",
    "build-prod": "ng build -c production --progress=false --aot=true",
    "CI_install": "yarn --silent --ignore-engines --link-duplicates --non-interactive --ignore-optional --frozen-lockfile",
    "rebuild-sass": "npm rebuild node-sass",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "browserlist": "npx browserslist --update-db"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/bite-industry/projects/beetask/front.git"
  },
  "author": "Bite Industry",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/bite-industry/projects/beetask/front/issues"
  },
  "homepage": "https://beeTask.io",
  "private": true,
